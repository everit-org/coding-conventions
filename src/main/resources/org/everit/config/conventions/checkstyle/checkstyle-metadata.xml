<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
  <rule-group-metadata name="Everit" priority="1700">

    <rule-metadata name="SuppressGeneratedHolder" internal-name="org.everit.config.conventions.checkstyle.SuppressGeneratedFilter" parent="TreeWalker"
      hasSeverity="false">
      <alternative-name internal-name="org.everit.config.conventions.checkstyle.SuppressGeneratedHolder" />
      <description>The analyzer part of SuppressGeneratedFilter. The two works together.</description>
    </rule-metadata>

    <rule-metadata name="SuppressGeneratedFilter" internal-name="org.everit.config.conventions.checkstyle.SuppressGeneratedFilter" parent="Checker"
      hasSeverity="false">
      <alternative-name internal-name="org.everit.config.conventions.checkstyle.SuppressGeneratedFilter" />
      <description>Filters functions and classes that are annotated with @Generated. It works only together with SuppressGeneratedHolder.</description>
    </rule-metadata>


  </rule-group-metadata>
</checkstyle-metadata>
    